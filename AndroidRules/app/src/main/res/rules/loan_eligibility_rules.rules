[CalculateDTIRule:
    (?applicant rdf:type foaf:Person)
    (?applicant schema:monthlyDebt ?debt)
    (?applicant schema:monthlyIncome ?income)
    quotient(?debt, ?income, ?dti)
    -> (?applicant ex:debtToIncomeRatio ?dti)
]

[EligibilityRule:
    (?applicant rdf:type foaf:Person)
    (?applicant ex:creditScore ?score)
    (?applicant ex:debtToIncomeRatio ?dti)
    greaterThan(?score, '600'^^xsd:int)
    lessThan(?dti, '0.35'^^xsd:float)
    -> (?applicant ex:loanEligibility "Eligible")
]

[HighDTIRule:
    (?applicant rdf:type foaf:Person)
    (?applicant ex:debtToIncomeRatio ?dti)
    greaterThanOrEqual(?dti, '0.35'^^xsd:float)
    -> (?applicant ex:loanEligibility "Not Eligible - High DTI")
]

[LowCreditRule:
    (?applicant rdf:type foaf:Person)
    (?applicant ex:creditScore ?score)
    lessThanOrEqual(?score, '600'^^xsd:int)
    -> (?applicant ex:loanEligibility "Not Eligible - Low Credit")
]